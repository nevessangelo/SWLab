[r0: (?s1 ?p ?o), (?s1 owl:sameAs ?s2), strConcat(?s2, ?l), regex(?l, "http://localhost:8080/resource/.*") -> (?s2 ?p ?o)]
[r1: (?s rdf:type dbo:Place) -> (?s rdf:type sch:Place)]

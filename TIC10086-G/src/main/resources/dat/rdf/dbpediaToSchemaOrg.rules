[r13: (?s geo:lat ?o1)
      , (?s geo:long ?o2)
      , makeSkolem(?sk, ?o1, ?o2)
      , uriConcat("http://swlab.ic.uff.br/resource/id2-",?sk, ?g) -> (?s sch:geo ?g)
                                                                      , (?t rdf:type sch:GeoCoordinates)
                                                                      , (?t sch:latitude ?o1)

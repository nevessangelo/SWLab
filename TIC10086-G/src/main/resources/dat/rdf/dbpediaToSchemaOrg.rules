[r0: (?s1 ?p ?o)
     , (?s1 owl:sameAs ?s2)
     , strConcat(?s2, ?l)
     , regex(?l, "http://swlab.ic.uff.br/resource/id2-.*") -> (?s2 ?p ?o)
                                                               , [(?s3 ?p2 ?s2) <- (?s3 ?p2 ?s1)]]
